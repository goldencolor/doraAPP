    function execNavRightCallback(value){
        var name = 'shared_mask';
        var page = 'shared_mask.html';
        var rect ={
            x: 0,
            y: 0,
            w: 'auto',
            h: 'auto'
        };
        var data = {id : id, frameName: "contentFrame"};
        var bounces = false;
        openNewFrame(name,page,bounces,rect,data);
    }

    function execShareWx(index){
        var scene = '';
        var contentUrl = shareUrl+"themesDetail/"+id;
        if(index == 1){
            scene = 'session';
        }else if(index == 2){
            scene = 'timeline';
        }
        shareWx(scene,contentUrl);
    }

    function shareWx(scene,contentUrl){
        var wx = api.require('wx');
        if(!isDefine(scene)){
            return;
        }
        wx.shareWebpage({
            apiKey: CONSTANT.WEIXINAPPKEY,
            scene: scene,
            title: "多啦衣梦主题系列",
            description: "点我了解详情!",
            thumb: 'widget://image/sharedLogo.png',
            contentUrl: contentUrl,
        }, function(ret, err){
            var msg = '未知错误';
            if(ret.status==true){
                msg = '分享成功';
            }else{
                if(err.code==-1){
                    msg = '未知错误';
                }else if(err.code==0){
                    msg = '成功';
                }else if(err.code==1){
                    msg = 'apiKey非法';
                }else if(err.code==2){
                    msg = '你取消了分享';
                }else if(err.code==3){
                    msg = '发送失败';
                }else if(err.code==4){
                    msg = '授权拒绝';
                }else if(err.code==5){
                    msg = '微信服务器返回的不支持错误';
                }else if(err.code==6){
                    msg = '当前设备未安装微信客户端';
                }else if(err.code==7){
                    msg = '注册SDK失败';
                }
            }
            toastMsg(msg);
        });
    }

    function execCloseSharedMask(frameName,index){
        index = parseInt(index);
        if(!isNaN(index)){
            //调用微信模块
            execShareWx(index);
        }
        api.closeFrame({
            name:frameName
        });
    }
