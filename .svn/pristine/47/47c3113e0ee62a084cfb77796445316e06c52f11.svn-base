    <style>
        .mainContent{
            background-color: #F8F8F8;
            padding: 0 0.6rem;
        }
        .clearMargin:before{
            content: '';
            display: table;
        }
        /*主题标题模版样式*/
        .themeHead{
            width: 100%;
        }
        .themeHead .progressive{
            margin-top: 0.6rem;
            min-height: 17.55rem;
        }

        /*1:1模版样式*/
        .oneToOne{
            width: 100%;
            background-color: #FFFFFF;
            margin-top: 0.6rem;
        }
        .oneToOne .progressive{
            min-height: 10.5rem;
        }


        /*16:9模版样式*/
        .sixToNine{
            width: 100%;
            background-color: #FFFFFF;
            margin-top: 0.6rem;
        }
        .sixToNine .img-holder{
            max-height: 17.55rem;
            overflow: hidden;
        }
        .sixToNine .discribleBox{
            width: 100%;
            text-align: center;
            height: 2.75rem;
            background-color: #FFFFFF;
        }
        .sixToNine .discribleBox .name{
            font-family: SourceHanSansCN-Normal;
            font-size: 0.5rem;
            line-height: 0.5rem;
            color: #464646;
            margin-top: 0.5rem;
        }
        .sixToNine .discribleBox .size{
            font-family: Arial-BoldMT;
            font-size: 0.6rem;
            line-height: 0.6rem;
            color: #666666;
            margin-top: 0.4rem;
        }
        .sixToNine .discribleBox .size span{
            margin: 0 0.25rem;
        }

        /*2:3模版样式*/
        .twoToThree{
            width: 100%;
            background-color: #FFFFFF;
            margin-top: 0.6rem;
        }
        .twoToThree .img-holder{
            max-height: 26.35rem;
        }

        .twoToThree .discribleBox{
            width: 100%;
            text-align: center;
            height: 2.75rem;
            background-color: #FFFFFF;
        }
        .twoToThree .discribleBox .name{
            font-family: SourceHanSansCN-Normal;
            font-size: 0.5rem;
            line-height: 0.5rem;
            color: #4A4A4A;
            margin-top: 0.5rem;
        }
        .twoToThree .discribleBox .size{
            font-family: Arial-BoldMT;
            font-size: 0.6rem;
            line-height: 0.6rem;
            color: #4A4A4A;
            margin-top: 0.4rem;
        }
        .twoToThree .discribleBox .size span{
            margin: 0 0.25rem;
        }
        /*小编模版样式*/
        .editor{
            margin-top: 0.6rem;
            width: 100%;
            background-color: #FFFFFF;
        }
        .editor .info{
            height: 3.45rem;
            border-bottom: 1px solid #F0F0F0;
        }
        .editor .info img{
            width: 1.75rem;
            height: 1.75rem;
            border-radius: 50%;
            margin:  0 0.5rem 0 1.05rem;
        }
        .editor .info .name{
            font-family: SourceHanSansCN-Normal;
            font-size: 0.6rem;
            line-height: 0.6rem;
            color: #3C3C3C;

        }
        .editor .info .small{
            font-family: SourceHanSansCN-Bold;
            font-size: 0.6rem;
            line-height: 0.6rem;
            color: #3C3C3C;
            margin-right: 1.05rem;
        }
        .editor .discribleBox{
            width: 100%;
            padding: 1.4rem 1.05rem;
        }
        .editor .discribleBox .title{
            font-family: SourceHanSansCN-Bold;
            font-size: 0.9rem;
            color: #454545;
            letter-spacing: -0.05rem;
            line-height: 1.4rem;
            text-align: justify;
        }
        .editor .discribleBox .text{
            font-family: SourceHanSansCN-Normal;
            font-size: 0.7rem;
            color: #777777;
            letter-spacing: 0;
            line-height: 1.4rem;
            text-align: justify;
        }
        /*侧滑模版样式*/
        .sideSlip{
            width: 100%;
        }
        .sideSlip .swiper-wrapper{
            margin: 0.75rem 0;
        }
        .sideSlip .item {
            width: 6.5rem;
            height: 12.6rem;
            background-color: #FFFFFF;

            margin-right: 0.75rem;
        }
        .sideSlip .item:last-child {
            margin-right: 0;
        }
        .sideSlip .item img {
            height: 9.75rem;
        }

        .sideSlip .item .discrible {
            width: 100%;
            height: 2.75rem;
            text-align: center;
            box-shadow: 0 2px 2px 0 rgba(227, 227, 227, 0.50);
        }
        .sideSlip .item .discrible .name {
            display: block;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            padding-top: 0.5rem;
            font-family: SourceHanSansCN-Normal;
            font-size: 0.5rem;
            color: #464646;
        }
        .sideSlip .item .discrible .size {
            display: block;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            padding-top: 0.4rem;
            font-family: Arial-BoldMT;
            font-size: 0.6rem;
            color: #666666;
            line-height: 0.6rem;
        }
        .sideSlip .item .discrible .size span {
            margin: 0 0.15rem;
        }
        /*商品列表模版样式*/
        .goodsList{
            width: 100%;
        }
        .goodsList .aui-row{
            background-color: #F8F8F8;
        }
        .goodsList .item{
            /*width: 8.4rem;*/
            margin-top: 0.75rem;
            padding: 0;
        }
        .goodsList .item:nth-child(odd){
            padding-right: 0.375rem;
        }
        .goodsList .item:nth-child(even){
            padding-left: 0.375rem;
        }
        .goodsList .item .progressive{
            height: 12.6rem;
            background-color: #FFFFFF;
        }

        .goodsList .item .discribleBox{
            width: 100%;
            background-color: #FFFFFF;
        }
        .goodsList .item .discribleBox .name{
            font-family: SourceHanSansCN-Normal;
            font-size: 0.5rem;
            line-height: 0.5rem;
            color: #464646;
            padding-top: 0.5rem;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
        .goodsList .item .discribleBox .size{
            font-family: Arial-BoldMT;
            font-size: 0.6rem;
            color: #4A4A4A;
            line-height: 0.6rem;
            padding-top: 0.4rem;
            padding-bottom: 0.75rem;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
        .goodsList .item .discribleBox .size span{
            margin: 0 0.15rem;
        }
        .item{
            position: relative;
        }
        .unavailable{
            font-size: 0.5rem;
            color: #FFFFFF;
            background: #9A9A9A;
            width: 2.5rem;
            height: 0.7rem;
            line-height: 0.7rem;
            text-align: center;
            border-radius:0 0 0 0.3rem;
            position: absolute;
            z-index: 1000;
            top:0;
            right: 0;
        }
        .goodsList .item:nth-child(odd) .unavailable{
            right: 0.375rem;
        }
    </style>
</head>
