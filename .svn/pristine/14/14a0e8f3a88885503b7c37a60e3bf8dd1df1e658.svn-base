	<!--尺码对照表模板-->
	<script id="sizeControl-container-template" type="text/x-dot-template">

		<tr class="theader">
			{{? it.count == 0 }}
				<td style="text-align: left">暂无</td>
			{{??}}
				<td>尺码</td>
			{{?}}
			{{~it.name:value:index}}
