	<!--试穿报告模板-->
	<script id="model-container-template" type="text/x-dot-template">
		<div class="title">试穿报告</div>
		<table class="clothe-size-table">
			<tr class="theader">
				<td>{{=it[0].name}}</td>
				{{~it[0].value:value:index}}
				<td>{{=value }}</td>
				{{~}}
		</tr>
		{{ for(var i = 1; i < it.length; i++) { }}
			{{? !it[i]._id}}
			<tr>
				<td>{{=it[i].name}}</td>
				{{~it[i].value:value:index}}
				{{? value == null}}
				<td>—</td>
				{{??}}
				<td>{{=value }}</td>
				{{?}}
				{{~}}
			</tr>
			{{??}}
			<tr>
				<td>{{=it[i].name}}</td>
				{{ for(var k = 0; k < it[i].value.length; k++) { }}
				{{ if(!it[i].value[k]) continue; }}
				{{? it[i].value[k].type == "model"}}
				<td>本页所示</td>
				{{?? it[i].value[k].img == null}}
				<td>—</td>
				{{??}}
				<td tapmode onclick="{{=getExecScript('goShowImg',[k,it[i].value[k].img.map(function(doc){return doc;}).toString()])}}">
