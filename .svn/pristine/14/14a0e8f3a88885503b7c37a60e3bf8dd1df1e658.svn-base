    <style>
		/*调整轮播样式开始*/
		.aui-slide-page-dot{
			background-color: #FFFFFF;
		}
		.aui-slide-page-active{
			background-color:#FF6100 !important;
		}
		/*调整轮播样式结束*/
		#UIScrollPicture{
			height: 28.1rem;
			position: relative;
		}
		/*分享按钮*/
		#shared-btn{
			width: 2.5rem;
			height: 2.5rem;
			line-height: 2.5rem;
			border-radius: 100%;
			position: absolute;
			bottom:-1rem;
			right: 0.5rem;
			background: #FFFFFF;
			box-shadow: 0px 2px 4px 0px rgba(188,188,188,0.50);
			font-size: 0.7rem;
			text-align: center;
			z-index: 9999;
		}

		#clothe-title{
			width: 100%;
			padding: 0.6rem 0 0.6rem 0.5rem;
			font-family: SourceHanSansCN-Normal;
			font-size: 1rem;
			color: #464646;
			background: #FAFAFA;
		}

		section{
			margin: 3.1rem 0.5rem 0 0.5rem;
		}
		section .title{
			font-family: SourceHanSansCN-Light;
			font-size: 1rem;
			color: #464646;
		}
		/*尺码*/
		#clothe-size-container{
			margin-top: 0.2rem;
		}
		.clothe-size{
			border: 1px solid #E6E6E6;
			border-radius: 2px;
			margin: 0 auto;
			width: 85%;
			height: 1.5rem;
			line-height: 1.5rem;
			text-align: center;
			font-family: SourceHanSansCN-Normal;
			font-size: 0.7rem;
			color: #E6E6E6;
		}
		.clothe-size.available{
			color: #7F7F7F;
			border: 1px solid #464646;
		}
		.clothe-size.active{
			color: #FF6100;
			border: 1px solid #FF6100;
		}
		#clothe-size-container .aui-col-xs-4{
			padding: 0.75rem 0 0 0;
		}
		#clothe-size-container .aui-col-xs-4:active{
			background-color: #FFFFFF !important;
		}
		/*尺码表*/
		.clothe-size-table{
			margin-top: 0.7rem;
			font-family: SourceHanSansCN-Light;
			font-size: 0.7rem;
			color: #4f4f4f;
			font-weight: 100;
			width: 100%;
		}
		.clothe-size-table tr{
			width: 100%;
		}
		.clothe-size-table td{
			width: 20%;
			max-width: 20%;
			text-align: center;
			/*white-space:nowrap;*/
			overflow:hidden;
			/*text-overflow:ellipsis;*/
			line-height: 1.4rem;
			font-size: 0.7rem;
			font-weight: 100;
		}
		/*#clothe-size-table tr td:nth-child(2){*/
			/*width: 30%;*/
		/*}*/
		.clothe-size-table .theader td,.clothe-size-table tr td:first-child{
			/*padding: 0 2rem 0 0;*/
			color: #969696;
		}
		.clothe-size-table .img-show{
			background-color:#ffffff !important;
			color: #FF6100;
			/*width: 2.5rem;*/
			margin-left: 25%;
			width: 50%;
			height: auto;
			border-radius: 0.2rem;
			border: 1px solid #FF6100;
		}
		/*商品信息*/
		#clothes-info-container{
			margin-top: 0.5rem;
			font-family: SourceHanSansCN-Light;
			font-size: 0.7rem;
			color: #A6A6A6;
		}
		#clothes-info-container .item{
			line-height: 1.5rem;
			font-weight: 100;
			color: #464646;
		}
		#clothes-info-container .item span{
			line-height: 1.5rem;
			display: inline-block;
			vertical-align: middle;
			color: #969696;
			width: 3rem;
			text-align: right;
		}
		/*#clothes-info-container .item span{*/
			/*display: block;*/
			/*min-width: 3.5rem;*/
			/*text-align:justify;*/
			/*text-align-last:justify;*/
		/*}*/

		/*底部按钮*/
		#clotheset-add-btn{
			width: 60%;
			float:left;
			height:2.8rem;
			line-height:2.8rem;
			background: #FF6100;
			color: #FFFFFF;
			font-family: SourceHanSansCN-Normal;
			font-size: 0.8rem;
			text-align: center;
		}
		#go-my-clotheset{
			width: 40%;
			float:left;
			height:2.8rem;
			line-height:2.8rem;
			font-family: SourceHanSansCN-Normal;
			font-size: 0.8rem;
			color: #646464;
			text-align: center;
		}
		#modelShowsContainer{
			margin-top: 0.9rem;
		}
		#clothe-img-container img{
			margin-bottom: 0.5rem;
		}

		/*品牌区域*/
		#brand-container section{
			margin-top: 1.5rem;
		}
		.brand-desc{
			padding-top: 1.5rem;
		}
		.brand-desc-text{
			font-family: SourceHanSansCN-Normal;
			font-size: 0.7rem;
			color: #464646;
			line-height: 0.7rem;
		}
		.brand-desc-text{
			font-family: SourceHanSansCN-Normal;
			font-size: 0.7rem;
			color: #464646;
			line-height: 0.7rem;
		}
		.brand-desc-more,.brand-desc-more span{
			font-family: SourceHanSansCN-Normal;
			font-size: 0.5rem;
			color: #474747;
			line-height: 0.5rem;
		}
		.icon-line{
			font-weight: 600;
			font-size: 0.7rem;
		}

		/*评论区域*/
		#comment-container section{
			margin-left: 0;
			margin-right: 0;
		}
		#comment-container .title{
			padding-left: 0.5rem;
		}
		#comment-container .line-1{
			height: 1px ;
			background-color: #F6F6F6;
		}
		.comment-item{
			margin: 0 0.5rem;
			padding: 1.125rem 0;
		}
		.comment-item .avatar{
			width: 1.9rem;
			height: 1.9rem;
			display: block;
			border-radius: 100%;
			margin-right: 0.55rem;
		}
		.comment-item .comment-header{
			height: 1.3rem;
		}
		.comment-item .comment-header .name{
			font-family: SourceHanSansCN-Normal;
			font-size: 0.6rem;
			color: #969696;
			margin-right: 0.75rem;
		}
		.comment-item .comment-header .time{
			font-family: SourceHanSansCN-Normal;
			font-size: 0.6rem;
			color: #969696;
			max-width: 150px;
			height: 0.9rem;
		}
		.comment-item .comment-header .icon-Stars{
			font-size: 0.5rem;
			margin: 0 0.15rem;
		}
		.comment-item .comment-text{
			padding: 0.1rem 0;
			font-family: SourceHanSansCN-Normal;
			font-size: 0.7rem;
			color: #464646;
			line-height: 1rem;
		}
		.comment-item .comment-img{
			padding: 0.1rem 0;
		}
		.comment-item .comment-img img{
			width: 2rem;
			min-height: 2rem;
		}
		.comment-more{
			width: 7rem;
			text-align: center;
			margin: 0 auto;
			margin-top: 1rem;
			font-family: SourceHanSansCN-Normal;
			font-size: 0.6rem;
			color: #464646;
			height: 1.5rem;
			line-height: 1.5rem;
			border: 1px solid #4A4A4A;
			border-radius: 2px;
		}
