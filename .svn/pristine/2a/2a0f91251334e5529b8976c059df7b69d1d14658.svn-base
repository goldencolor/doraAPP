        <div class="swiper-span">
            已持有
        </div>
        {{??}}
        <div class="swiper-delete" tapmode onclick=" {{='deleteBag(\''+ ('bag-id-'+i) +'\',\''+ i +'\',\''+it[i].size+'\',\''+it[i]._id+'\',\''+it[i].online+'\',\''+it[i].barcode+'\')'}}">
            <i class="duola-iconfont icon-delete" ></i>
        </div>
        {{?}}
    </div>
    {{ } }}
