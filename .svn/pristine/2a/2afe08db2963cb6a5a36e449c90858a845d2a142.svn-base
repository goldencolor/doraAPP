<style>
    .my-header {
        background: url('../image/mybg.png') no-repeat;
        background-size: cover;
        min-height: 11rem;
        position: relative;
    }
    .my-info {
        position: relative;
        padding: 1rem 0;
        width: 100%;
        bottom: 0;
        text-align: center;
        vertical-align: center;
    }
    .my-info img {
        width: 4rem;
        height: 4rem;
        border-radius: 50%;
    }
    .my-info .info-desc{
        padding: 0.7rem 0;
    }
    .info-desc p{
        color: #ffffff;
        font-size: 0.8rem;
    }
    .info-desc p.nickname{
        font-size: 0.9rem;
    }
    .my-menu {
        position: absolute;
        width: 100%;
        height: 2.25rem;
        line-height: 2.25rem;
        left: 0;
        bottom: 0;
        text-align: center;
        background: #ffffff;
        filter:Alpha(opacity=50);
        background:rgba(255, 255, 255, 0.2) none repeat scroll 0 0 !important;
        color: #ffffff;
        font-size: 0.8rem;
    }
    .my-menu > div:active {
        background: #ffffff;
        filter:Alpha(opacity=50);
        background:rgba(255, 255, 255, 0.2) none repeat scroll 0 0 !important;
    }
    .info-list .aui-iconfont{
        color: #FF6100 !important;
        font-size: 0.75rem;
    }
    .info-list .aui-list .aui-list-item-title{
        color: #707070 !important;
        font-size: 0.75rem !important;
    }
    .info-list .icon-service{
        font-size: 0.45rem;
        color: #FF6100 !important;
    }
    .info-list .icon-Pickup{
        font-size: 0.5rem;
        color: #FF6100 !important;
    }
    #my-version{
        font-size: 0.65rem;
    }
</style>
<body>
<div class="aui-content my-header">
    <div class="my-info">
        <img id="user-header" src="../image/defaultHead.jpg" onclick="changeAvatar(this)">
        <div class="info-desc flex flex-center flex-vertical">
            <p class="nickname" id="nickname"></p>
            <p class="mobile" id="mobile"></p>
        </div>
    </div>
    <div class="my-menu">
        <div class="aui-col-xs-4" tapmode onclick="openMsg(this)">我的消息</div>
        <div class="aui-col-xs-4" tapmode onclick="openInfo(this)">个人信息</div>
        <div class="aui-col-xs-4" tapmode onclick="openPwd(this)">密码安全</div>
    </div>
</div>
