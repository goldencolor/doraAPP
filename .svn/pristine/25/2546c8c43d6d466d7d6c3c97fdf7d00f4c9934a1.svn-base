<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="viewport"
          content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>退回</title>
    <link rel="stylesheet" type="text/css" href="../css/api.css"/>
    <link rel="stylesheet" type="text/css" href="../css/2_0/aui_2_0.css"/>
    <link rel="stylesheet" type="text/css" href="../css/2_0/rem.css"/>
    <link rel="stylesheet" type="text/css" href="../css/2_0/util.css"/>
    <link rel="stylesheet" type="text/css" href="../css/fonts/duola_iconfont2.css"/>
    <link rel="stylesheet" type="text/css" href="../css/uzui.css"/>
    <style>
        body, html {
            background: #F7F7F7;
        }
        .waterfall {
            background-color: #FFFFFF;
        }

        .waterfall .item img {
            width: 100%;
        }

        .waterfall .item img {
            height: 8.5rem;
            position: relative;
        }

        .waterfall .item .add-bag {
            position: relative;
            text-align: center;
            width: 100%;
            height: 2rem;
            margin-top: 0.5rem;
        }

        .waterfall .item .footer .name {
            text-align: center;
            padding-top: 0.3rem;
        }

        .waterfall .item .header .size {
            font-size: 0.6rem;
            min-width: 1rem;
            padding: 0 0.2rem;
            height: 1rem;
            line-height: 1rem;
            text-align: center;
            background: #000000;
            color: #FFFFFF;
            position: absolute;
            bottom: 0;
            border-radius: 0 0.2rem 0 0;
        }

        .waterfall .item .header .delete div {
            width: 2rem;
            height: 2rem;
            line-height: 2rem;
            position: relative;
            left: 35%;
            background: #000000;
            opacity: 0.69;
            border-radius: 4px;
        }

        .aui-radio:checked {
            background-color: #FF6100;
            border: solid 1px #FF6100;
        }

    </style>
</head>
<body>
<div class="aui-grid">
    <div id="cloudset-container" class="waterfall">
    </div>
</div>
</body>
<script type="text/javascript" src="../script/api.js"></script>
<script type="text/javascript" src="../script/url.js"></script>
<script type="text/javascript" src="../script/uz.js"></script>
<script type="text/javascript" src="../script/umeng_sdk.js"></script>
<script type="text/javascript" src="../script/doT_min.js"></script>


<script id="cloudset-container-template" type="text/x-dot-template">
    {{ for(var i = 0; i < it.length; i++) { }}
    <div class="item aui-col-xs-4">
        <div class="header">
            <img src="../image/hold.jpg" class="{{='cloudset-img-'+it[i].id}}" width="100%"/>
            <div class="size">{{=it[i].size == "J"? "均码":it[i].size}}</div>
            <div class="label flex flex-space-between">
                {{? it[i].reversed == "YES"}}
                <div class="design">寄回中</div>
                {{?}}
            </div>
        </div>
        <div class="footer flex flex-space-between flex-vertical-center">
            <div class="name flex-1 aui-ellipsis-1">{{=it[i].name}}</div>
        </div>
    </div>
    {{ } }}
</script>
<script type="text/javascript">
    var addressList = [];
    var addressNum = 0;
    var date = 0;
    var time = 0;
    var count = 0;
    apiready = function () {
        init();
    };
    function init() {
        _loadingShow();
        var bodyt = {
            "page": 1,
            "pageSize": 20
        };
        var url = serviceUser + 'orders/current';
        var headers = _getAjaxHeaders(true, true);
        var data = _getAjaxData(bodyt);
        var extra = {url: url};
        _ajaxData(url, 'post', headers, data, initSuccessCallback, initErrorCallback, 0, extra);
    }
    function initSuccessCallback(ret, extra) {
        console.log("order_two ret == "+JSON.stringify(ret))
        _loadingHide();
        if (ret.statusCode == '200') {
            count = ret.count || 0;
            var list = [];
            for (var i = 0; i < ret.list.length; i++) {
                if (ret.list[i].reversed == "YES") {
                    list.push(ret.list[i]);
                }
            }

            doHtml("cloudset-container", "cloudset-container-template", list);
            for (var i = 0; i < ret.list.length; i++) {
                var url = colthPath + ret.list[i].img + CONSTANT.PICTUREOSS.CLOTHESLIST;
                _imgCacheUrl2(url, '.cloudset-img-' + ret.list[i].id);
            }
        } else if (ret.statusCode == '10104') {
            toLogin();
        } else {
            toastMsg(ret.msg);
        }
    }
    function initErrorCallback(err, extra) {
        _loadingHide();
        toastMsg(err.msg);
    }
</script>
</html>