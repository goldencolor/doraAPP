			toLogin();
		}else{
			toastMsg(ret.msg);
		}
	}
	function choose(id){
		var id = id;
		var url = serviceNew + 'user/address/setDefault/'+id;
		var headers = _getAjaxHeaders(true,true);
		var data = _getAjaxData();
		var extra = null;
		_ajaxData(url,'post',headers,data,chooseSuccessCallback,chooseErrorCallback,0,extra);
	}
