	function init(){
		_loadingShow();
		var url = serviceUser + 'address/list';
		var headers = _getAjaxHeaders(token,true);
		var data = _getAjaxData();
		var extra = null;
		_ajaxData(url,'post',headers,data,initSuccessCallback,initErrorCallback,0,extra);
	}

