	function myAjaxSuccessCallback(ret,extra){
		_loadingHide();
		if(ret.statusCode=='200'){
			api.sendEvent({
				name: 'removeBag',
				extra: {key: "YES" }
			});
			api.openWin({
				name: 'orderSuccess',
				url: 'winOrder.html',
				pageParam:{
					name: 'orderSuccess',
					page: 'orderOk',
					title: '配送成功'
				}
			});
		}else{
			toastMsg(ret.msg)
		}
	}

	function myAjaxErrorCallback(err,extra){
