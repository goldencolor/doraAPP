    //拼接头像url
    function getAvatar(avatar,isWxAvatar){
        if(isTrue(isWxAvatar)){
            return avatar;
        }else if(avatar && avatar.indexOf('../') == -1){
            return [CONSTANT.PICPRE.RES,avatar].join('');
        }else{
            return '';
        }
    }
