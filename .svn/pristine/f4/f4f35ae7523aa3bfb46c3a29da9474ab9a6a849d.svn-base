			api.closeWin({name:'Wallet'});
		}
		api.openWin({
			name: 'Wallet',
			url: 'html/win.html',
			pageParam:{
				page: 'Wallet',
				title: '我的账户',
				type: type
			}

		});
	}
	function opencategory(){
		if(isOpencategory){
			isOpencategory = false;
			api.closeFrame({
				name: 'category'
			});
		}else {
			isOpencategory = true;
			api.openFrame({
				name: 'category',
				url: 'html/closet/category.html',
				rect: {
					x:0,
					y:70,
					w:"auto",           //宽度，若传'auto'，页面从x位置开始自动充满父页面宽度
					h:"auto"
				},
				pageParam: {
				},
				vScrollBarEnabled: false
			});
		}
	}

	//点击搜索
	function goSearch(){
		sendEventUmeng(CONSTANT.UMENGEVENT.SEARCH_BTN, {from: '云衣橱'});
		var gStyle = $api.getStorage('gStyle',gStyle);
		if(gStyle){
			openSearchFrame(gStyle);
		}else{
